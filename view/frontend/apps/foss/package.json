{
  "name": "@hydris/foss",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "private": true,
  "scripts": {
    "dev": "expo start",
    "dev:android": "expo run:android",
    "dev:web": "expo start --web",
    "build:android": "cross-env CI=1 NODE_ENV=production APP_ENV=production bun run build:android:exec",
    "build:android:staging": "cross-env CI=1 NODE_ENV=production APP_ENV=staging bun run build:android:exec",
    "build:android:dev": "cross-env CI=1 NODE_ENV=development APP_ENV=development bun run build:android:exec",
    "build:android:exec": "bunx expo prebuild --clean --platform android && cd android && ./gradlew clean assembleRelease",
    "build:web": "cross-env APP_ENV=production expo export -p web --output-dir ./build --clear",
    "build:web:staging": "cross-env APP_ENV=staging expo export -p web --output-dir ./build --clear",
    "serve": "expo serve ./build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
